# /etc/fstab: static file system information.
#
# Use 'blkid' to print the universally unique identifier for a
# device; this may be used with UUID= as a more robust way to name devices
# that works even if disks are added and removed. See fstab(5).
#
# <file system> <mount point>   <type>  <options>       <dump>  <pass>
# / was on /dev/sdc2 during installation
UUID=125bceec-caf0-4ac6-b1dc-f0982f3ae076 /               ext4    errors=remount-ro 0       1
# /boot/efi was on /dev/sdc1 during installation
UUID=8B48-7805  /boot/efi       vfat    defaults        0       1
# swap was on /dev/sdc3 during installation
UUID=925ebf17-230f-43ea-aa51-81a5fed2e6ce none            swap    sw              0       0
/dev/sdb1       /media/usb0     auto    rw,user,noauto,noatime,discard  0       0
/dev/sdc1       /media/usb1     auto    rw,user,noauto  0       0
/dev/sdc2       /media/usb2     auto    rw,user,noauto  0       0
/dev/sdc3       /media/usb3     auto    rw,user,noauto  0       0
